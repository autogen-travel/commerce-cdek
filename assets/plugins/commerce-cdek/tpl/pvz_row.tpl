<div 
data-cdek-type="pvzItem" 
data-cdek-code="[+code+]" 
data-cdek-name="[+name+]" 
data-cdek-hours="[+work_time+]" 
data-cdek-metro="[+nearest_station+]" 
data-cdek-address="[+address+]" 
data-cdek-coords="[[+coordY+],[+coordX+]]">
	<div class="cdek_list_item">
		<div class="pvz_info">
			<div class="cdek_pvz_cell cdek_pvz_name">[+name+]</div>
			<div class="cdek_pvz_cell cdek_pvz_address">[+address+]</div>
			<div class="cdek_pvz_cell cdek_pvz_metro">[+nearest_station+]</div>
			<div class="cdek_pvz_cell cdek_pvz_hours">[+work_time+]</div>
			<button data-cdek-action="setPvz" class="cdek-pvz-button">Выбрать</button>
		</div>
	</div>
</div>